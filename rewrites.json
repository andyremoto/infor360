{
  "rewrites": [
    {
      "source": "/*",
      "target": "/index.html",
      "status": "200"
    }
  ],
  "redirects": [
    {
      "source": "/404.html",
      "target": "/index.html",
      "status": "200"
    },
    {
      "source": "/200.html",
      "target": "/index.html",
      "status": "200"
    }
  ]
}